    <questiontext format="html">
      <text><![CDATA[<p dir="ltr" style="text-align: left;">¿Cuáles son las tres características clave de una línea de productos?</p>]]></text>
    </questiontext>
    <generalfeedback format="html">
      <text></text>
    </generalfeedback>
    <defaultgrade>1</defaultgrade>
    <penalty>0.3333333</penalty>
