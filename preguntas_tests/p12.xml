    <shownumcorrect/>
    <answer fraction="100" format="html">
      <text><![CDATA[<p dir="ltr" style="text-align: left;">La reutilización, la personalización, y la flexibilidad.</p>]]></text>
      <feedback format="html">
        <text></text>
      </feedback>
    </answer>
    <answer fraction="-33.33333" format="html">
      <text><![CDATA[<p dir="ltr" style="text-align: left;">La escalabilidad, la inversión inicial pequeña, y la gestión de la variabilidad.</p>]]></text>
      <feedback format="html">
        <text></text>
      </feedback>
    </answer>
    <answer fraction="-33.33333" format="html">
      <text><![CDATA[<p dir="ltr" style="text-align: left;">La dependencia de componentes, la escalabilidad, y la calidad de los componentes.</p>]]></text>
      <feedback format="html">
        <text></text>
      </feedback>
    </answer>
    <answer fraction="-33.33333" format="html">
      <text><![CDATA[<p dir="ltr" style="text-align: left;">La uniformidad (<em>uniformity</em>), la separación de conceptos (<em>separation of concerns</em>), y la ocultación de información <em>(information hidding</em>).</p>]]></text>
      <feedback format="html">
        <text></text>
      </feedback>
